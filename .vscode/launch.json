{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Electron Main",
            "runtimeExecutable": "${workspaceFolder:Hubble}/node_modules/.bin/electron",
            "program": "${workspaceFolder:Hubble}/main.js",
            "windows": {
                "runtimeExecutable": "${workspaceFolder:Hubble}/node_modules/.bin/electron.cmd"
            },
            "env": {
                "NODE_ENV": "development"
            },
            "outputCapture": "std"
        }
    ]
}